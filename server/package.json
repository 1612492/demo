{
  "name": "server",
  "version": "0.0.1",
  "main": "./dist/app.js",
  "scripts": {
    "dev": "nodemon",
    "dev:ts": "ts-node ./src/app.ts",
    "build": "tsc",
    "start": "node ./dist/app.js",
    "db:up": "db-migrate up",
    "db:down": "db-migrate reset",
    "lint": "eslint . --ext ts",
    "format": "prettier --ignore-unknown --write ."
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.1",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
